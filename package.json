{
  "name": "focusaint-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "focusaint - Habit Tracking & Focus Management Platform | Monorepo",
  "keywords": [
    "productivity",
    "habit-tracking",
    "focus-management",
    "focus",
    "habits"
  ],
  "license": "MIT",
  "author": "focusaint Team",
  "scripts": {
    "start:frontend": "npm run --prefix frontend start",
    "start:backend": "npm run --prefix backend start",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "dev:frontend": "npm run --prefix frontend dev",
    "dev:backend": "npm run --prefix backend dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "concurrently \"cd frontend && npm i\" \" cd backend && npm i\""
  },
  "dependencies": {
    "@emotion/is-prop-valid": "latest",
    "@vercel/analytics": "1.6.1",
    "bcryptjs": "3.0.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "framer-motion": "12.26.2",
    "jsonwebtoken": "9.0.3",
    "lucide-react": "0.562.0",
    "mongoose": "9.1.4",
    "next": "^16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "tw-animate-css": "1.4.0"
  },
  "devDependencies": {
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
